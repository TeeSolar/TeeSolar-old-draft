<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
   <mime-type type="application/x-teesolar-package">
     <comment>TeeSolar Package</comment>
     <magic priority="50">
       <match value="TUA1" type="string" offset="0"/>
     </magic>
     <glob pattern="*.tup"/>
   </mime-type>
   <mime-type type="application/x-teeworlds-map">
     <comment>TeeWorlds/Ninslash Map</comment>
     <magic priority="50">
       <match value="DATA" type="string" offset="0"/>
     </magic>
     <glob pattern="*.map"/>
   </mime-type>
</mime-info>
